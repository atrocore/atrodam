{
  "name": "atrocore/dam",
  "description": "AtroDAM. Open source DAM application.",
  "require": {
    "ext-fileinfo": "*",
    "atrocore/core": "~1.4.50"
  },
  "conflict": {
    "atrocore/pim": "<1.5.23 || >=1.6.0"
  },
  "autoload": {
    "psr-4": {
      "Dam\\": "app/"
    }
  },
  "extra": {
    "treoId": "Dam",
    "name": {
      "default": "DAM"
    },
    "description": {
      "default": "The module allows to manage digital assets of preconfigured types and to link them with other entities such as products etc."
    },
    "tags": [
      "manage",
      "usability",
      "files",
      "attachments"
    ]
  }
}
