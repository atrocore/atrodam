{
  "name": "atrocore/dam",
  "description": "AtroDAM. Open source DAM application.",
  "require": {
    "ext-fileinfo": "*",
    "atrocore/core": "~1.2.8",
    "atrocore/dam-common": "~1.2.0",
    "imal-h/pdf-box": "~1.3.1"
  },
  "autoload": {
    "psr-4": {
      "Dam\\": "app/"
    }
  },
  "extra": {
    "treoId": "Dam",
    "name": {
      "default": "DAM"
    },
    "description": {
      "default": "The module allows to manage digital assets of preconfigured types and to link them with other entities such as products etc."
    },
    "tags": [
      "manage",
      "usability",
      "files",
      "attachments"
    ]
  }
}
